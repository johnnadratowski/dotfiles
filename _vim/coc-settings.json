{
  "languageserver": {
    "coffee": {
      "command": "coffeesense-language-server",
      "filetypes": ["coffee"]
    }
  },
  "coc.source.omni.enable": true,
  "coc.source.omni.filetypes": ["styl", "stylus"],
  "less.completion.completePropertyWithSemicolon": false,
  "css.completion.completePropertyWithSemicolon": false,
  "scss.completion.completePropertyWithSemicolon": false,
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/opt/homebrew/bin/black"
  "coc.preferences.formatOnSaveFiletypes": ["python"]
}
